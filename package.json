{
  "name": "basketball-simulator",
  "version": "1.0.0",
  "description": "Basketball Simulator",
  "engines": {
    "node": "16.x"
  },
  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/server.js --ignore client",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZackeryArsement/basketball-simulation.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZackeryArsement/basketball-simulation/issues"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "dotenv": "^16.0.0",
    "http-proxy-middleware": "^2.0.4"
  },
  "dependencies": {
    "gapi-script": "^1.2.0",
    "concurrently": "^7.0.0"
  }
}
